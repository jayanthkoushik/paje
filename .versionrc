{
  "bumpFiles": [
    {
      "filename": ".version",
      "type": "plain-text"
    }
  ],
  "scripts": {
    "postbump": "sed -i '' \"s|jayanthkoushik/paje:v.*$|jayanthkoushik/paje:v`cat .version`'|\" action.yml",
    "precommit": "git add action.yml",
    "postcommit": "git tag -f v`cat .version | cut -d. -f1`"
  },
  "commit-all": true,
  "skip": {
    "changelog": true
  }
}
